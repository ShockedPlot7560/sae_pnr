<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="332.0" prefWidth="743.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="107.0" prefWidth="765.0">
         <children>
            <Label text="Indice">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <ComboBox fx:id="indiceComboBox" prefHeight="51.0" prefWidth="680.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </ComboBox>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </VBox>
      <VBox prefHeight="108.0" prefWidth="743.0">
         <children>
            <Label text="Commune">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="communeComboBox" onAction="#disableEditingCommune" prefHeight="25.0" prefWidth="410.0" />
                        <Button fx:id="switchButtonCommune" mnemonicParsing="false" onAction="#enableEditingCommune" text="Ajouter" />
                     </children>
                  </HBox>
                  <HBox fx:id="addCommuneBox" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="nomCommune" maxWidth="411.0" minWidth="150.0" prefHeight="25.0" prefWidth="411.0" promptText="Nom commune" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </VBox>
      <VBox prefHeight="108.0" prefWidth="743.0">
         <children>
            <Label text="Lieu Dit">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="lieuDitComboBox" onAction="#disableEditingLieuDit" prefHeight="25.0" prefWidth="411.0" />
                        <Button fx:id="switchButtonLieuDit" mnemonicParsing="false" onAction="#enableEditingLieuDit" text="Ajouter" />
                     </children>
                  </HBox>
                  <HBox fx:id="addLieuDitBox" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="nomLieuDit" maxWidth="412.0" minWidth="150.0" prefHeight="25.0" prefWidth="412.0" promptText="Nom lieu dit" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </VBox>
   </children>
</VBox>
